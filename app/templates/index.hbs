<div class="container col-mid-10">
  {{new-question saveQuestion="saveQuestion"}}
  <table class="table table-hover">
    <thead>
      <tr>
        <th>
          Question:
        </th>
        <th>
          Asked By:
        </th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |question|}}
        <tr>
          <td>
            {{#link-to 'question' question.id}}
            {{question.title}}
            {{/link-to}}
          </td>
          <td>
            {{question.user}}
          </td>
        </tr>
    {{/each}}
    </tbody>
  </table>
</div>
